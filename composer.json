{
  "name": "blackford/twilio-bundle",
  "type": "symfony-bundle",
  "description": "Symfony 5+ / PHP 8+ wrapper for the official Twilio SDK v6+",
  "keywords": [
    "Twilio"
  ],
  "homepage": "https://github.com/mblackford/BlackfordTwilioBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthew Blackford",
      "email": "matthew.blackford@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4 | ^7.0",
    "twilio/sdk": "^7.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 | ^11.0.3",
    "php-coveralls/php-coveralls": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Blackford\\TwilioBundle\\": ""
    }
  }
}
